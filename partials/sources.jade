#sources

  #pictures
    i.fa.fa-cloud

  h1 {{system.name}}

  .channel(ng-repeat="source in sources", ui-sref="source({sourceId: source.id})")
    .box
      span.helper
      img(ng-if="source.picture", ng-src="{{'images/sources/' + source.id + '/' + source.picture}}")
      .footer
        span {{source.name}}